{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "standard",
    "parser": "babel-eslint",
    "rules": {
        "semi": [
            "error",
            "always"
        ]
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ]
}